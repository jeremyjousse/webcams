{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint \"{,!(node_modules|public)/**/}*.{js,ts,svelte}\"",
    "fix-code": "prettier-eslint --write 'src/**/*.{js,jsx,ts,svelte}' ",
    "fix-styles": "prettier-stylelint --write '{src,public}/**/*.{css,scss}' ",
    "start": "sirv public",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.13.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "fast-check": "^2.6.0",
    "prettier": "2.1.2",
    "prettier-stylelint": "^0.4.2",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  }
}
